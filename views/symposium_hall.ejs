    <style>
        .compatibility-message {
            display: none;
        }
        
        @media (orientation: portrait) {
            html,
            body {
                position: fixed;
                overflow: hidden;
                background-color: #000;
                max-width: 100%;
            }
            .compatibility-message {
                display: block;
                position: fixed;
                z-index: 999999;
                width: 100%;
                height: 100%;
                background: #13110e;
                text-align: center;
            }
            .compatibility-message img {
                max-width: 100%;
            }
        }
        
        body {
            width: 100%;
            background-image: linear-gradient(to right, #FEFEFF, #E8E8E8);
        }
        
        .header_img {
            width: 100%;
            margin-top: 3.5rem;
            position: relative;
        }
        
        @media(min-width:2560px) {
            .header_img {
                width: 134%;
                margin-top: 3.5rem;
                position: relative;
            }
        }
        
        @media(min-width:3840px) {
            .header_img {
                width: 200%;
                margin-top: 3.5rem;
                position: relative;
            }
        }
        
        .btn1 {
            position: relative;
            top: 35%;
            left: -18%
        }
        
        .btn2 {
            position: relative;
            top: 35%;
            left: -18%
        }
        
        .separator {
            display: flex;
            align-items: center;
            text-align: center;
        }
        
        .separator::before,
        .separator::after {
            content: '';
            flex: 1;
            border-bottom: 2px solid #007acc;
        }
        
        .text {
            padding: 30px;
        }
        
        .text1 {
            margin: 11vh 5vh 0 0;
        }
        
        @media (max-width:3840px) {
            .text1 {
                margin: 4vh 5vh 0 0;
            }
        }
        
        @media (max-width:2560px) {
            .text1 {
                margin: 6vh 5vh 0 0;
            }
        }
        
        @media (max-width:1920px) {
            .text1 {
                margin: 6vh 5vh 0 0;
            }
        }
        
        .text1 {
            margin: 9vh 5vh 0 0;
        }
        
        @media (max-width:1024px) {
            #mainsession {
                margin-top: 14vh;
            }
            #breakout {
                margin-top: 7vh;
                margin-bottom: 5vh;
            }
            #breakout_2 {
                margin-top: 5vh;
            }
            .text {
                padding: 30px 0px 30px 10px;
            }
            .bg1 {
                background-size: 100% 29vh;
            }
            .bg2 {
                background-size: 100% 24vh;
            }
            .bg3 {
                background-size: 100% 19vh;
            }
            .text1 {
                margin: 5vh 2vh 0 0;
            }
            .text1 img {
                width: 100%;
                height: auto;
            }
        }
        @media (max-width:768px){
    .btn1{
        top: 65px;
        left: -28px;
        margin-right: 15px;
        
        
      }
      .btn2{
        top: 65px;
        left: -28px;
        margin-right: 15px;
      }
      
  }
  @media (max-width:760px){
    .text-center h2{
                margin-top: 70px;
              
               font-size: 40px !important;
           }
      
  }
        @media (max-width:768px) {
            #mainsession {
                margin-top: 14vh;
            }
            .text {
                padding: 30px 0px 30px 10px;
            }
            .text1 {
                margin: 10vh 2vh 0 0;
            }
            .text1 img {
                width: 100%;
                height: auto;
            }
           
            .text-left {
                font-size: 15px;
            }
            .bottom-text {
                font-size: 15px;
            }
            .back-img {
                top: 5%;
            }
        }
        
        @media (max-width:450px) {
            .text-center h2{
                margin-top: 70px;
              
               font-size: 30px !important;
           }
        }
        
        .footer {
            min-height: 60px;
            /* .push must be the same height as .footer */
            background-image: linear-gradient(to right, #FEFEFF, #E8E8E8);
        }
        .back-img {
            position: absolute;
            top: 13%;
        }
    </style>
</head>
