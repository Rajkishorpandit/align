  <style>
    .compatibility-message {
        display: none;
    }
    
    @media (orientation: portrait) {
        html,
        body {
            overflow: hidden;
            background-color: #000;
            max-width: 100%;
        }
        .compatibility-message {
            display: block;
            position: fixed;
            z-index: 999999;
            width: 100%;
            height: 100%;
            background: #13110e;
            text-align: center;
        }
        .compatibility-message img {
            max-width: 100%;
        }
    }
    /*------------hotsport css-----------------*/
    
    .hotspots {
        width: 25px;
        height: 25px;
        background-color: #3ab3ef;
        border-radius: 50%;
        position: absolute;
        animation: wave 1s infinite;
        cursor: pointer;
    }
    
    .zoom {
        left: 36%;
        bottom: 41%;
    }
    
    .inv {
        right: 32%;
        bottom: 39%;
    }
    
    .support_chat {
        right: 22.5%;
        bottom: 39%;
    }
    
    .Enquiries {
        right: 12%;
        bottom: 33%;
    }
    
    .hotspots::before {
        content: attr(text);
        width: max-content;
        position: absolute;
        background-color: #222222dd;
        color: #fff;
        left: 50%;
        transform: translateX(-50%);
        top: 30px;
        padding: 8px 20px;
        font-size: 14px;
        border-radius: 3px;
        display: none;
    }
    
    .hotspots::after {
        content: "";
        border-color: transparent transparent #222222dd;
        border-width: 0 8px 6px;
        border-style: solid;
        position: absolute;
        top: 24px;
        left: 50%;
        transform: translateX(-50%);
        display: none;
    }
    
    .hotspots:hover::before,
    .hotspots:hover::after {
        display: block;
    }
    
    @media (max-width: 768px) {
        .hotspots {
            width: 12px;
            height: 12px;
        }
        .zoom {
            bottom: 43%;
        }
        .inv {
