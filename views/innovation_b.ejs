      @media (orientation: portrait) {
          html,
          body {
              overflow: hidden;
              background-color: #000;
              max-width: 100%;
          }
          .compatibility-message {
              display: block;
              position: fixed;
              z-index: 999999;
              width: 100%;
              height: 100%;
              background: #13110e;
              text-align: center;
          }
          .compatibility-message img {
              max-width: 100%;
          }
